{
    "name": "creof/geo-obj",
    "type": "library",
    "description": "Library for geography and geometry objects",
    "keywords": ["geometry", "geography", "spatial", "geo", "coordinate", "point", "multipoint", "linestring", "multilinestring", "polygon", "multipolygon"],
    "authors": [
        {
            "name":  "Derek J. Lambert",
            "email": "dlambert@dereklambert.com"
        },
        {
            "name": "Jon West",
            "email": "ddproxy@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "creof/geo-parser": "dev-develop",
        "creof/wkt-parser": "dev-develop",
        "creof/wkb-parser": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload":          {
        "psr-4": {"CrEOF\\Geo\\Obj\\": "lib/"}
    },
    "autoload-dev":      {
        "psr-4": {"CrEOF\\Geo\\Obj\\Tests\\": "tests/"}
    }

}
